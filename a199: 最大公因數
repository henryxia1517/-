#include <iostream>

using namespace std;

int main(int argc, char** argv) {
	int a, b;
   cin >> a >> b ;
    
        while( a!=0 and b!=0 )
        {
            if( a >= b )
            {
                a = a%b;
            }
            else if( b > a )
            {
                b = b%a;
            }
        }

        if( a >= b )
        {
            cout << a << endl;
        }
        else
        {
            cout << b << endl;
        }
    
	return 0;
}
